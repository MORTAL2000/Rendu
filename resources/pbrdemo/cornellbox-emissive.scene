* scene:
	probe: rgbcube: default_cube
	irradiance: default_shcoeffs

* background:
	color: 0.0,0.0,0.0

* camera:
	position: 0.0,1.0,2.5
	center: 0.0,1.0,1.0
	up: 0.0,1.0,0.0
	fov: 1.3
	planes: 0.01,20.0

* directional:
	direction: 0.0,-0.3,-1.0
	intensity: 0.8,0.8,0.7
	shadows: true
	
* object:
